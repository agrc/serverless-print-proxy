{
  "name": "serverless-print-proxy",
  "version": "1.1.0",
  "description": "A Google Cloud Function for switching out quad words in print requests to ArcGIS Server",
  "repository": "https://github.com/agrc/serverless-print-proxy",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test-ci": "npm run lint && jest",
    "test": "jest --watch",
    "start": "functions-framework --target=printproxy"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "Scott Davis <stdavis@utah.gov>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "request": "2.88.2"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "1.5.1",
    "eslint": "6.8.0",
    "eslint-config-agrc": "1.3.0",
    "eslint-plugin-html": "6.0.2",
    "jest": "25.5.4",
    "supertest": "4.0.2"
  }
}
